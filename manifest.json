{
  "name": "Copy token",
  "version": "1.0",
  "description": "Copy your Fedid token to the clipboard!",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "clipboardWrite"],
  "browser_action": {
    "default_popup": "copy_token.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
